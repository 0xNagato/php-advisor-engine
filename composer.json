{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "andreiio/blade-remix-icon": "^3.2",
        "ariaieboy/filament-currency": "^1.2",
        "ashallendesign/short-url": "^7.10",
        "bezhansalleh/filament-shield": "^3.1",
        "calebporzio/sushi": "^2.5",
        "chillerlan/php-qrcode": "^5.0",
        "codeat3/blade-google-material-design-icons": "^1.19",
        "codeat3/blade-govicons": "^1.4",
        "doctrine/dbal": "^3.8",
        "filament/filament": "^3.1",
        "guzzlehttp/guzzle": "^7.2",
        "hirethunk/verbs": "^0.3.0",
        "itsgoingd/clockwork": "^5.1",
        "jenssegers/agent": "^2.6",
        "larabug/larabug": "^3.0",
        "laravel-notification-channels/twilio": "^3.3",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.23",
        "laravel/pail": "^1.1",
        "laravel/pulse": "^1.0@beta",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "leandrocfe/filament-apex-charts": "^3.0",
        "livewire/livewire": "^3.4",
        "livewire/volt": "^1.6",
        "lorisleiva/laravel-actions": "^2.7",
        "opcodesio/log-viewer": "^3.8",
        "owenvoke/blade-fontawesome": "^2.6",
        "pelmered/filament-money-field": "^1.0",
        "propaganistas/laravel-phone": "^5.2",
        "rappasoft/laravel-authentication-log": "^4.0",
        "ryangjchandler/blade-tabler-icons": "^2.3",
        "sentry/sentry-laravel": "^4.4",
        "spatie/laravel-data": "^4.0",
        "spatie/laravel-markdown": "^2.5",
        "spatie/laravel-pdf": "^1.4",
        "spatie/opening-hours": "^3.0",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "stechstudio/filament-impersonate": "^3.6",
        "stripe/stripe-php": "^13.16",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "tapp/filament-timezone-field": "^3.0",
        "torann/geoip": "^3.0",
        "vonage/client": "^4.0",
        "ysfkaya/filament-phone-input": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^7.0",
        "laradumps/laradumps": "^3.0",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/phpinsights": "^2.11",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-horizon-watcher": "^1.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
